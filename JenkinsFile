pipeline {
    agent any
    tools{
    maven'MAVEN'
    }
    stages {
        stage('code checkout') {
            steps {
        git changelog: false, poll: false, url: 'https://github.com/Aniket10720/Javawebapp.git'
            }
        }
        stage('code build') {
            steps {
                sh 'mvn build'
            }
        }
        stage('test') {
            steps {
                sh 'mvn testing'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy to nexus '
            }
        }
    }
}
