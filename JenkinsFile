                echo 'code checkout'
            }
        }
        stage('code build') {
            steps {
                echo 'code build'
            }
        }
        stage('test') {
            steps {
                echo 'testing'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
    }
}
